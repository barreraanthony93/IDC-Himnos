{
  "expo": {
    "name": "IDCHimnos",
    "slug": "IDCHimnos",
    "version": "2.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon-01.png",
    "userInterfaceStyle": "light", 
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#121838"
    },
    "updates": {
      "fallbackToCacheTimeout": 3000,
      "url": "https://u.expo.dev/36ed0ece-3cf9-487e-9bb8-fa4ddebf466b"
    },
    "build": {
      "preview": {
        "channel": "preview"
      },
      "production": {
        "channel": "production"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.iglesiadecristoenelgin.idchimnos",
      "buildNumber": "2.0.3",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.iglesiadecristoenelgin.idchimnos",
      "versionCode": 6,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121838"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "36ed0ece-3cf9-487e-9bb8-fa4ddebf466b"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      ["expo-build-properties",
      {
        "android": {
          "compileSdkVersion": 33,
          "targetSdkVersion": 33,
          "buildToolsVersion": "33.0.0"
        },
        "ios": {
          "deploymentTarget": "13.0"
        }
      }
    ]
    ]
  }
}
